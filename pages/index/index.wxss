/**index.wxss**/
.sections{
  display: flex;
  flex-direction: column;
}
.section-title{
  padding: 10rpx 30rpx;
  font-size: 32rpx;
  color: #1c2438;
  background: #f8f8f9;
}
.row{
  padding: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-style: none none solid none;
  border-width: 1rpx;
  border-color: #dddee1;
  font-size: 32rpx;
  color: #778899;
  font-weight: bold;
}
.row-title{
  font-size: 36rpx;
  color: #1c2438;
}